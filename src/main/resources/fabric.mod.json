{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Vanillify",
  "description": "A library designed to allow vanilla clients to connect to a heavily modified fabric server!",
  "authors": [
    "BlazeCode",
    "Cosmix"
  ],
  "contact": {
    "homepage": "https://github.com/BlazeCodeNet",
    "sources": "https://github.com/BlazeCodeNet/VanillifyMod"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "nnet.blazecode.vanillify.VanillifyMod"
    ]
  },
  "mixins": [
    "vanillify.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.17.x"
  },
  "suggests": {
    "another-mod": "*"
  }
}
